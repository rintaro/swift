#define DEFINE_DIAGNOSTIC_MACROS
#include "DefineDiagnosticMacros.h"

//===----------------------------------------------------------------------===//

ERROR(ide_async_in_nonasync_context, none,
      "async %0 used in a context that does not support concurrency", (DeclName))

ERROR(ide_cross_actor_reference, none,
      "actor-isolated %0 can only be referenced from inside the actor", (DeclName))

WARNING(ide_redundant_import, none,
        "module %0 is already imported", (DeclName))

NOTE(ide_redundant_import_indirect, none,
     "module %0 is already imported via another module import", (DeclName))

ERROR(ide_var_init_own_reference, none,
     "variable %0 cannot be used within its own initial value", (DeclName))

WARNING(availability_softdeprecated, Deprecation,
        "%select{getter for |setter for |}0%1 will be deprecated"
        "deprecated%select{| in %3%select{| %5}4}2%select{|: %6}6",
        (unsigned, DeclName, bool, StringRef, bool, llvm::VersionTuple,
         StringRef))

//===----------------------------------------------------------------------===//

#define UNDEFINE_DIAGNOSTIC_MACROS
#include "DefineDiagnosticMacros.h"
